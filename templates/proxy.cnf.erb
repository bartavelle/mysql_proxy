[mysql-proxy]
admin-address = 0.0.0.0:<%=scope.lookupvar('mysql_proxy::admin_port')%>
proxy-skip-profiling = true
admin-password=<%=scope.lookupvar('mysql_proxy::admin_password')%>
admin-username=administrator
proxy-address=0.0.0.0:<%=scope.lookupvar('mysql_proxy::proxy_port')%>
proxy-backend-addresses=<%=scope.lookupvar('mysql_proxy::mysql_ip')%>:<%=scope.lookupvar('mysql_proxy::mysql_port')%>
max-open-files=1024
user=mysqlproxy
proxy-lua-script=/usr/lib/mysql-proxy/lua/cachefilter.lua
log-file=/var/log/mysql-proxy/daemon.log
keepalive=true
